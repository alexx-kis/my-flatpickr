{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "globals": {
        "gsap": "readonly",
        "Splide": "readonly",
        "mixitup": "readonly",
        "ScrollTrigger": "readonly"
    },
    "rules": {
        "no-console": "warn",
        "no-unused-vars": "warn",
        "prefer-arrow-callback": "warn"
    }
}